package traning;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		int a = 10;
		
		a = a + 1;
		System.out.println(a);
		
		a++;
		System.out.println(a);
		
		++a;
		System.out.println(a);
		
		a = 10;
		System.out.println(a);
		
		int b = ++a + 10;
		System.out.println(a);
		
	}

}
