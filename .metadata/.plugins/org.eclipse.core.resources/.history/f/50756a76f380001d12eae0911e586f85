package Test;

abstract class Animal {
	abstract void cry();
}

class Cat extends Animal {
	void cry() {
		System.out.println("정현우");
	}
}

class Dog extends Animal {
	void cry() {
		System.out.println("아치");
	}
}

public class AbstractTest {

	public static void main(String[] args) {
		Animal a1 = new Cat();
		a1.cry();

		Animal a2 = new Dog();
		a2.cry();

	}

}
