package mypack;

import java.util.Arrays;

public class Test {

	public static void main(String[] args) {
		
		int[] array = new int[] { 1, 2, 3 };
		
	}
	
	private static void printArray(int a) {
		System.out.println(Arrays.toString(a));
	}
	
	private static void modifyData(int[] a) {
		a[0] = 4;
		a[1] = 5;
		a[2] = 6;
	}
}
