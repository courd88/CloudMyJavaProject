package myaddressbook;

public class Person {

private String name;
private String phone;

public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getPhone() {
	return phone;
}
public void setPhone(String phone) {
	this.phone = phone;
}
 public void print() {
	 System.out.printf("%s\t%\n", name, phone);
 }
	

}
