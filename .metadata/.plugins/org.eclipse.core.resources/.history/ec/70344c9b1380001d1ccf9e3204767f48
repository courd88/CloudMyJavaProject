package polymorphism;

class A {
	int m = 10;

	void abc() {
		System.out.println("A.abc() is called");
	}
}

class B extends A {
	int n = 20;

	void bcd() {
		System.out.println("B.bcd() is called");
	}
}

public class InheritanceSample {

	public static void main(String[] args) {
		B b = new B();
		System.out.println("B.n = " + b.n);
		b.bcd();

	}

}
