package mypack;

import java.util.Arrays;
import java.util.Scanner;

public class Test {

	public static void main(String[] args) {

		int[][] a1 = new int[3][];
		a1[0] = new int[2];
		a1[1] = new int[] { 3, 4, 5 };
		a1[2] = new int[] { 6, 7, 8 };
		System.out.println(Arrays.deepToString(a1));
		a1[0][0] = 1;
		a1[0][1] = 2;
		
	}
	
}
