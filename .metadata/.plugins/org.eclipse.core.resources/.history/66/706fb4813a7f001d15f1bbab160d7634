package mypack;

public class Gugudan {
	
	int dan;
	
	Gugudan() {
		this.dan = 2;
	}
	Gugudan(int dan) {
		this.dan = dan;
	}
	
	
	void print() {
		System.out.println("-------------");
		System.out.printf("%d ë‹¨\n");
		for (int i = 1; i < 10; i ++) {
			System.out.printf("%d * %d = %d \n", dan, i, dan * i);
		}
	}

}
