package mypack;

import java.util.Arrays;

public class qoduf {

	public static void main(String[] args) {

		int[][] a1 = new int[2][3];
		a1[0][0] = 1;
		a1[0][1] = 2;
		a1[0][2] = 3;
		a1[1][0] = 4;
		a1[1][1] = 5;
		a1[1][2] = 6;
		System.out.println(Arrays.deepToString(a1));
		
		int[][] a2 = new int[2][3];
		a2[0] = new int[] {1, 2, 3 };
		a2[1] = new int[] {4, 5, 6 };
		System.out.println(Arrays.deepToString(a2));
		
		

	}
}