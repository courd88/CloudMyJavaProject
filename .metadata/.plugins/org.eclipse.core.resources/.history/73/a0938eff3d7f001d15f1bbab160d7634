package mypack;

public class Gugudan {
	
	private int dan;
	
	Gugudan() {
		this.dan = 2;
	}
	Gugudan(int dan) {
		this.dan = dan;
	}
	
	void setDan(int dan) {
		this.dan = dan;
		
	}
	
	
	void print() {
		System.out.println("\n===============");
		System.out.printf("%dë‹¨\n" + this.dan);
		System.out.println("\n===========");
		for (int i = 1; i < 10; i ++) {
			System.out.printf("%d * %d = %2d \n", dan, i, dan * i);
		}
	}

}
