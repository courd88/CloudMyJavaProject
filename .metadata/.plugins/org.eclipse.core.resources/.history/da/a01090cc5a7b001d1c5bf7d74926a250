package mypack;

import java.util.Arrays;

public class qoduf {

	public static void main(String[] args) {

		int[] a = new int[10];
		System.out.println(Arrays.toString(a));
		
		
	    for (int i = 0; i < a.length; i ++) {
	    	a[i] = i * 10;
	    }
	    System.out.println(Arrays.toString(a));
	    
	   
	
	

}
}