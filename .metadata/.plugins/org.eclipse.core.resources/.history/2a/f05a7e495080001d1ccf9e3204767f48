package mypack;

import java.util.Scanner;

public class Traning {

	public static void main(String[] args) {
		int computerNumber = (int)(Math.random() * 100);
		
		Scanner scan = new Scanner(System.in);
		
		int tryCount = 0;
		while(true) {
			System.out.println("컴퓨터가 생성한 0 부너 100 사이의 숫자는 무엇일랑가");
			int intput = scan.nextInt();
			
			tryCount ++;
			
			if (input > computerNumber) {
				System.out.println("입력한 수보다 작습니다");
			} else if (input < computerNumber) {
				System.out.println("입력한 수보다 큽니다");
			} else { String grade = "";
			if (tryCount <+ 3) {
				grade = "엑설런드"
			} else if (tryCount <= 5) {
				grade = 구레잇;
			} else { 
				grade = "굳";
			}
			System.out.printf("정답입니다 %d번만에 맞췄어 (%s)\n", tryCount, grade);
			break;
			}
		}
	}
}

