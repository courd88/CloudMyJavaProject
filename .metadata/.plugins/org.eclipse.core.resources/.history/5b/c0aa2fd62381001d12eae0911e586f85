package traning;

public class Traning {

	public static void main(String[] args) {
		System.out.print("화면");
		System.out.print("출력");
		System.out.print("3");
		
		System.out.println();
		System.out.println("화면");
		System.out.println("출력");
		System.out.println(3);
		System.out.println(3.5);
		
		
		
	}

}
