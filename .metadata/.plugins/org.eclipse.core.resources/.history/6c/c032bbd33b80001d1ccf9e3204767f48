package mybank;

public class Account {
	// 필드는 외부에서 접금 못 하도록 private 접근 제한자를 설정
	private String accountNo; // 계좌번호
	private String name;	   // 계좌주
	private int balance;		//잔액
	
	//각 필드의 값을 조회하고 설정 가능하도록 getter/setter를 정의
	public String getAcrrountNo() {
		return accountNo;
	}
	public void setAcrrountNo(String acrrountNo) {
		this.accountNo = acrrountNo;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getBalance() {
		return balance;
	}
	public void setBalance(int balance) {
		this.balance = balance;
	}
	
	
	
	

}
